o:Sass::Tree::RootNode:@template"%/* Global Classes / Wordpress Specific */
.aligncenter    { text-align: center; margin: 0 auto; }
.alignleft      { float: left; }
.alignright     { float: right; }
img.alignleft     { padding-right: 5px;}
img.alignright    { padding-left: 5px; }
.clear        { width:100%; float:left; clear:both;}


/* =============================================================================
                   Default Styling 
   ========================================================================== */

body {
  @include linear-gradient(45deg, $colour1 25%, $colour2 25%, $colour2 50%, $colour1 50%, $colour1 75%, $colour2 75%, $colour2);
    background-size: 150px 150px;
    font-family: $sans;
    font-weight: 300;
    font-size: 18px;
    line-height: 25px;
    color: $navy;
}

body:not(.home) header {
    margin: 0;
}

.wrapper{
  @include layout;
  background:$white;
  padding: 5%;
}


a {
  text-decoration: none;
  font-weight: 400;
  color: $white;
}

button{
  outline: none;
  border:none;
  background: $navy;
  color: $white;
  text-transform: uppercase;
  border-radius: 4px;
  padding: 16px 22px;
  white-space: nowrap;
  @include transition (all 0.6s ease-in-out);
}

header{
  display: block;
  margin: 25% auto 25%;
}

.logoWrapper{
  max-width: 800px;
  display: block;
  margin:0 auto;
  padding: 0;

}

.innerWrapper {
  width: 90%;
  margin: 0 auto;
  clear: both;
}


.centerContent{
  width:80%;
  margin: 0 auto;
  clear: both;
}

/* =============================================================================
                   CONTACT SECTION 
   ========================================================================== */
.contact{
  @include layout;
  background: $orange;
  padding: 10px;
  height:175px;

  h2{
    padding: 50px 0;
  }

  h3{
    font-size: 20px;
    margin-bottom: 5%;
    display: inline-block;
  }

 a{
  font-weight: bold;
  text-decoration: none;
  color: $navy;

    &:hover{
      color: $white;
    }

    &.close{
      position: relative;
      float: right;
      width: 36px;
      height: 36px;


    }
  }

  ul{
    margin: 0;
    padding: 33px 50px;
   

    li{
      position: relative;
      padding: 0 3% 0 0;
      display: inline-block;

      span{
        display: none;
        opacity: 0;
        @include transition (all 1.0s ease-in-out);
      }

      &:hover span{
        position: absolute;
        top: -52px;
        left: -67px;
        display: block;
        background-color:$white;
        font-weight: bold;
        font-size: 13px;
        line-height: 15px;
        margin: 0;
        padding:6px;
        text-align:center;
        width:180px;
        border-radius:10px;
        opacity: 1;
      }

      &:hover span:after{
        position: absolute;
        display: block;
        content: "";  
        border-color: $white transparent transparent transparent;
        border-style: solid;
        border-width: 10px;
        height:0;
        width:0;
        position:absolute;
        bottom:-19px;
        left:6em;
      }
     
       
      svg{
        fill:$navy;
        width: 60px;
        height: 60px;
        position: relative;
        top:12px;
        vertical-align: baseline;

          &:hover{
            fill:$white;
          }
        }
    }
  }    
  
} //.contact


/* =============================================================================
                   ABOUT SECTION 
   ========================================================================== */
.about{
  background:$yellow;
  padding: 1% 1% 0 1%;
  @include layout;

    h1{
    text-align: center;
    
    }

    h3{
      text-align: center;
      text-transform: none;
      font-size: 28px;
      line-height:30px;
      margin-bottom: 0;
    }


    img{
      position: relative;
      margin-top: -13%;
      left: 78%;
      bottom: -6px;
    }

}


.hire{
  float: right;
  
    &:hover{
      background:$yellow;
      color: $navy;
    }
}

/* =============================================================================
                   Before Work - Circles 
   ========================================================================== */
.infographics{
  @include layout;
  background:$purple;
  padding: 3%;
  color: $white;
}

.infographicWrap{
 float: left;
 width: 23%;
 margin-right: 2%;
 text-align: center;
 padding: 4% 0;

  h4{
    margin: 5px 0 0 0;
    font-size: 21px;
  }

  p{
    margin: 0;
    text-align: left;
  }

   .circle{
     border-radius: 50%;
     width: 200px;
     height: 200px;
     border: 3px solid $white;
     margin: 0 auto;
   }

   svg{
     width: 100px;
     height: 100px;
     fill:$white;
     transform-origin: top left;
     margin: 25%;
     
   }

   .info{
     line-height: 1.5em;
     font-size: 0.9em;
     margin: 0 auto; 
   }

} //.circleWrapper

/* =============================================================================
                   Work Section 
   ========================================================================== */

ul.filters{
  list-style: none;
  margin: 0;
  padding: 0 4px;
  text-align: center;
  clear: both;

    li{
      padding: 0;
      margin: 0 1px;
      display: inline-block;

        button{
          white-space: nowrap;
          border-radius: 2px;
          padding: 6px 8px;
          font-size: 14px;
          color:$white;
          font-family: $sans;
          text-transform: uppercase;
          font-weight: 400;
          background:$navy;
          outline: none;
          border: 2px solid transparent;
          @include transition (all 0.6s ease-in-out);


          &:hover{
            background:$purple;
          }
        }
    }
}


.workItems{
  margin: 0 auto 10%;
  padding: 0;
  list-style: none;
  text-align: center;


    li{
      display: inline-block;
      width: 300px;
      margin: 0;
      padding: 1%;
      text-align: left;
      position: relative;
    }

    figure{
      margin: 0;
      position: relative;
      overflow: hidden;
      @include shadow;

        img{
          max-width: 100%;
          display: block; 
        }

        &:hover figcaption{
          @include transform(translateX(0));
        
        }
    }

    figcaption {
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      padding: 12px;
      background: $navy;
      color: $white;
      @include transform(translateX(-100%));
      @include transform-origin(center left);
      @include transition (all 0.6s ease-in-out);

        h3{
          font-weight: 800;
          text-align: center;
          font-size: 20px;
          line-height: 22px;
          letter-spacing: 0;
          margin: 0;
          color: $purple;    
        }

        a{
          color: $purple;
          font-weight: bold;
          @include transition (all 0.6s ease-in-out);

          &:hover{
            color:$white;
          }
        }

        a.checkButton{
          position: absolute;
          bottom: 0;
          left: 0;
          width: 100%;
          text-align: center;
          padding: 2% 8px;
          display: inline-block;
          background: $purple;
          color: $white;
        }

        p{
          
          font-size: 16px;
          line-height: 21px;
          margin: 0;

          &.sub{
            font-size: 12px;
          }

        }

    }//figcaption

} //.workItems

.isotope-item {
  z-index: 2;
}

.isotope-hidden.isotope-item {
  pointer-events: none;
  z-index: 1;
}


/* =============================================================================
                   SERVICES 
   ========================================================================== */
.spacer{
  @include layout;
  padding: 5% 0;

    h2{
      margin: 0;
    }
}
.services{
  a{
    border-bottom: 1px solid $blue;
  }

}

.service{
  @include shadow;


    h3.serviceHeader{
      border:none;
      border-radius: 0;
      font-weight: 300;
      font-size: 1.5em;
      text-align: center;
      padding: 10px 0;
      margin: 0;
      background:$white;
      border-bottom: 1px solid #AAA;
      box-shadow: 0 6px 9px -4px rgba(0, 0, 0, 0.4);

        &.ui-accordion-header-active{
          font-weight: bold;
          border-bottom: 3px solid $blue;
        }
    } 

}

.serviceContent.ui-widget-content {
 padding: 4%;
 background:$white; 
 font-size: .9em;
 font-weight: 300;
 line-height: 20px;
 border: none;
 border-radius: 0;

  .quote{
    float: right;
    
      a{
        color:$white;
      }
      
      &:hover{
        background:$blue;
        
        a{
        color:$navy;
       }
      } 
  }

    h4, p, li{
      @extend body;
      background:$white;
    }

    h4{
      display: inline-block;
      font-size: 1em;
      font-weight: bold;
    }

    a{
      color: $blue;
      @include transition (all 0.6s ease-in-out);
      
      &:hover{
        color:$navy;
      }
    }

    ul{
      list-style-type: none;
      margin: 0;
      padding: 0;


      }

      li{

        &:before{
        font-family: $fa;
        content: "\f00c";
        color:$blue;
        padding-right: 5px;
        font-size: 20px;

      }

    }

}//.service

/* =============================================================================
                   Footer 
   ========================================================================== */

footer {
  background:$white;
  padding: 1% 0;
  text-align: center;
  clear:both;
}


